<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartcity_test4">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartCity_test4"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.personal.acvitity.FeedbackActivity" android:label="意见反馈" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".ui.personal.acvitity.OrderActivity" android:label="订单列表" android:parentActivityName=".MainActivity"/>
        <activity android:name=".ui.personal.acvitity.ChangeActivity" android:parentActivityName=".MainActivity" android:label="修改密码"/>
        <activity android:name=".ui.personal.acvitity.UserInfoActivity" android:label="个人详情" android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ui.personal.acvitity.LoginActivity"
            android:label="登陆页面"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.home.activity.AllServiceActivity"
            android:label="全部服务"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".guide.GuideActivity"
            android:label="引导页" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>